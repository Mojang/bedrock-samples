{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.zombie_horse.v1.and.v2": {
      "part_visibility": [
        { "*saddle*": "query.is_saddled" },
        { "SaddleMouthLine": "query.is_saddled && query.has_rider" },
        { "SaddleMouthLineR": "query.is_saddled && query.has_rider" },
        { "Bag*": false },
        { "MuleEar*": false }
      ],
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.default" }
      ],
      "textures": [ "Texture.zombie", "Texture.markings_none", "Texture.armor_none" ]
    }
  }
}
