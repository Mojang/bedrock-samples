{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.skeleton_horse.v4": {
      "part_visibility": [
        { "Saddle": false },
        { "Bit*": false },
        { "Bridle": false },
        { "Reins*": false },
        { "Bag*": false },
        { "MuleEar*": false }
      ],
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [{ "*": "Material.default" }],
      "textures": [
        "query.is_baby ? Texture.baby_skeleton: Texture.skeleton",
        "Texture.markings_none",
        "Texture.armor_none"
      ]
    }
  }
}
