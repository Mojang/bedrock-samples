{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.cow.v3": {
      "arrays": {
        "textures": {
            "Array.textures": [
                "Texture.default",
                "Texture.warm",
                "Texture.cold"
            ],
            "Array.baby_textures": [
                "Texture.baby_default",
                "Texture.baby_warm",
                "Texture.baby_cold"
            ]
        },
        "geometries": {
          "Array.geos": [
            "Geometry.default",
            "Geometry.warm",
            "Geometry.cold"
          ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Array.geos[v.index]",
      "materials": [ { "*": "v.is_cold ? Material.cold : Material.default" } ],
      "textures": [ "query.is_baby ? Array.baby_textures[v.index] : Array.textures[v.index]" ]
    }
  }
}
