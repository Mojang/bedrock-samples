{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.rabbit.v2": {
      "arrays": {
        "textures": {
          "Array.skins": [ "Texture.brown", "Texture.white", "Texture.black", "Texture.white_splotched", "Texture.gold", "Texture.salt" ],
           "Array.baby_skins": [ "Texture.baby_brown", "Texture.baby_white", "Texture.baby_black", "Texture.baby_white_splotched", "Texture.baby_gold", "Texture.baby_salt" ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "query.get_name == 'Toast' ? query.is_baby ? Texture.baby_toast: Texture.toast : query.is_baby ? Array.baby_skins[query.variant] : Array.skins[query.variant]" ]
    }
  }
}