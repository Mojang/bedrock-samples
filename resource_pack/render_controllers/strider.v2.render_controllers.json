{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.strider.v2": {
      "arrays": {
        "textures": {
          "Array.base": [
            "Texture.default",
            "Texture.saddled"
          ],
          "Array.suffocated": [
            "Texture.suffocated",
            "Texture.suffocated_saddled"
          ],
          "Array.baby": [
            "Texture.baby_default",
            "Texture.baby_suffocated"
          ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [
        { "*": "Material.default" }
      ],
      "textures": [
        "query.is_baby ? Array.baby[query.is_shaking] : query.is_shaking ? Array.suffocated[query.is_saddled] : Array.base[query.is_saddled]"
      ]
    }
  }
}
