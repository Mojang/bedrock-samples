{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.pig.v4": {
      "arrays": {
        "textures": {
          "Array.default": [
            "Texture.default",
            "Texture.warm",
            "Texture.cold"
          ],
          "Array.baby": [
            "Texture.baby_default",
            "Texture.baby_warm",
            "Texture.baby_cold"
          ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "query.is_baby ? Array.baby[v.index] : Array.default[v.index]" ]
    }
  }
}
