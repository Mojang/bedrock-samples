{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.nautilus": {
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "query.is_baby ? Texture.baby : Texture.default" ],
      "part_visibility": [
        { "head": "!variable.has_armor" }
      ]
    },
    "controller.render.nautilus_saddle": {
      "geometry": "Geometry.saddle",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.saddle" ],
      "part_visibility": [
        { "*": false },
        { "head": "q.is_saddled" }
      ]
    },
    "controller.render.nautilus_armor": {
      "part_visibility": [
        { "*": false },
        { "head": "variable.has_armor" }
      ],
      "arrays": {
        "textures": {
          "Array.armor": [
            "Texture.armor_none",
            "Texture.armor_copper",
            "Texture.armor_iron",
            "Texture.armor_gold",
            "Texture.armor_diamond",
            "Texture.armor_netherite"
          ]
        }
      },
      "geometry": "Geometry.armor",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.armor[variable.armor_texture_slot]" ]
    }
  }
}