{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.nautilus": {
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "query.is_baby ? Texture.baby : Texture.default" ],
      "part_visibility": [
        { "body": "!variable.has_armor" }
      ]
    },
    "controller.render.nautilus_saddle": {
      "geometry": "Geometry.saddle",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.saddle" ],
      "part_visibility": [
        { "*": false },
        { "head": "q.is_saddled" }
      ]
    }
  }
}