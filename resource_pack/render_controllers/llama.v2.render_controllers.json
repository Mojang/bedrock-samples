{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.llama.v2": {
      "arrays": {
        "textures": {
          "Array.base": [
            "Texture.creamy",
            "Texture.white",
            "Texture.brown",
            "Texture.gray"
          ],
          "Array.baby_base": [
            "Texture.baby_creamy",
            "Texture.baby_white",
            "Texture.baby_brown",
            "Texture.baby_gray"
          ],
          "Array.decor": [
            "Texture.decor_none",
            "Texture.decor_white",
            "Texture.decor_orange",
            "Texture.decor_magenta",
            "Texture.decor_light_blue",
            "Texture.decor_yellow",
            "Texture.decor_lime",
            "Texture.decor_pink",
            "Texture.decor_gray",
            "Texture.decor_silver",
            "Texture.decor_cyan",
            "Texture.decor_purple",
            "Texture.decor_blue",
            "Texture.decor_brown",
            "Texture.decor_green",
            "Texture.decor_red",
            "Texture.decor_black"
          ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "part_visibility": [{ "chest*": "query.is_chested" }],
      "materials": [{ "*": "Material.default" }],
      "textures": [
        "query.is_baby ? Array.baby_base[query.variant] : Array.base[query.variant]",
        "(query.mark_variant == 1 && variable.DecorTextureIndex == 0) ? query.is_baby ? Texture.baby_decor_wandering_trader : Texture.decor_wandering_trader : query.is_baby ? Texture.decor_none : Array.decor[variable.DecorTextureIndex]",
        "Texture.decor_none"
      ]
    }
  }
}
