{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.axolotl.v2": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.lucy",
            "Texture.cyan",
            "Texture.gold",
            "Texture.wild",
            "Texture.blue"
          ],
          "Array.baby_skins": [
            "Texture.baby_lucy",
            "Texture.baby_cyan",
            "Texture.baby_gold",
            "Texture.baby_wild",
            "Texture.baby_blue"
          ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [
        { "*": "Material.default" },
        { "*arm": "Material.limbs" },
        { "*leg": "Material.limbs" }
      ],
      "textures": [
        "query.is_baby ? Array.baby_skins[query.variant] : Array.skins[query.variant]"
      ]
    }
  }
}
