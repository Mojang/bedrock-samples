    "controller.render.player.map.persona": {
      "arrays": {
        "geometries": {
          "Array.geos": [ "Geometry.default", "Geometry.animated_face" ]
        }
      },
      "geometry": "Array.geos[query.is_persona_or_premium_skin]",
      "materials": [ { "*": "Material.animated" } ],
