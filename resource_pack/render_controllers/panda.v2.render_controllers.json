{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.panda.v2": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.lazy",
            "Texture.worried",
            "Texture.playful",
            "Texture.brown",
            "Texture.weak",
            "Texture.aggressive"
          ],
          "Array.baby_skins": [
            "Texture.baby_default",
            "Texture.baby_lazy",
            "Texture.baby_worried",
            "Texture.baby_playful",
            "Texture.baby_brown",
            "Texture.baby_weak",
            "Texture.baby_aggressive"
          ]
        }
      },
      "geometry": "query.is_baby ? Geometry.baby : Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "query.is_baby ? Array.baby_skins[query.variant] : Array.skins[query.variant]" ]
    }
  }
}
